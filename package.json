{
  "name": "autolog",
  "version": "1.0.0",
  "description": "App that list various parts from a car that helps you determine when you should replace a part based on average Kms lifetime",
  "main": "index.js",
  "scripts": {
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/index.css",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/index.css",
    "start": "npm run watch:css && react-scripts start",
    "dev": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test"
  },
  "author": "David Journo",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.7"
  }
}
